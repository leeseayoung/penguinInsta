<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
  
  <mapper namespace="com.penguin.penguinInsta.comment.repository.CommentRepository">
  
    	<!-- 댓글 추가 기능 -->
	  	<insert id="insertComment" parameterType="map">
	  		INSERT INTO 
	  				`comment`
	  			(
	  			
		  			`userId`
		  			,`postId`
					,`content`
					
							
	  			)
	  			VALUE
	  			(
	  				#{userId}
	  				, #{postId}
	  				, #{content}
	  			
	  				
	  			
	  			)
	  	
	  	
	  	
	  	</insert>
  
  
  </mapper>